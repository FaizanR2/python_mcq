def Q1():
    print('  1. What is the capital of Turkey')
    print('  A: Ankara')
    print('  B: Islamabad')
    print('  C: Istanbul')
    print('  D: Moscow')
    ans = input('Enter the Answer: ').upper()
    if len(ans) > 1:
        print('Please Enter the letter from A to D')
    if ans == 'A':
        print('Correct Answer')
    elif ans == 'B':
        print('Incorrect Answer')
    elif ans == 'C':
        print('Incorrect Answer')
    elif ans == 'D':
        print('Incorrect Answer')
    else:
        print('Invalid input, Please enter the letter from A to D')
        
def Q2():
    print('\n 2. What is the capital of Pakistan')
    print('   A: Ankara')
    print('   B: Islamabad')
    print('   C: Istanbul')
    print('   D: Moscow')
    ans = input('Enter the Answer: ').upper()
    if len(ans) > 1:
        print('Please Enter the letter from A to D')
    if ans == 'B':
        print('Correct Answer')
    elif ans == 'A':
        print('Incorrect Answer')
    elif ans == 'C':
        print('Incorrect Answer')
    elif ans == 'D':
        print('Incorrect Answer')
    else:
        print('Invalid input, Please enter the letter from A to D')
        
